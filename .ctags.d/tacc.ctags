--Python-kinds=+cfmv-i
--langmap=Python:+(__init__)
--langmap=c++:+.tin
--langmap=c++:+.itin
--langdef=tacc
--langmap=tacc:.tac
--regex-tacc=/^[ \t]*([A-Za-z0-9_]+)[ \t]*:/\1/d,definition/
--regex-tacc=/^[ \t]*([A-Za-z0-9_]+)[ \t]*;/\1/e,enumerators/
--regex-tacc=/^[ \t]*([A-Za-z0-9_]+)[ \t]*;/\1_/e,enumerators/
--extras=+fq
--fields=+im
--exclude=.git
